  "compilerOptions": {
    "target": "ES2020",
    "module": "CommonJS",
    "moduleResolution": "node",
    "strict": true,
    "esModuleInterop": true,
    "resolveJsonModule": true,
    "noEmit": true,
    "lib": ["ES2020"],
    "skipLibCheck": true,
    "types": ["node"]
  },
  "include": ["electron/**/*"]
}
